package demo04;

import java.util.Scanner;

public class demo043 {

	public static void main(String[] args) {
		int primes=50;
		int count = 0;
		int n=2;
		System.out.println("50¸öËØÊıÎª£º");
		while(count<=primes)
		{
			boolean flag=true;
			for(int i=2; i<n-1;i++)
			{
				if(n%i==0)
				{
					flag=false;
//					break;
				}
			}
			if(flag)
			{
				count++;
				if(count % 10 == 0)
				{
					System.out.println(n);
				}
				else
				{
					System.out.print(n + " ");
				}
				n++;
			}
		}
	}
}