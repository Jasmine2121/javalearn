package demo02;

import java.util.Scanner;

public class demo024 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int number;
		number = in.nextInt();
		do
		{
			int digit = number % 10;
			System.out.print(digit);
			number = number / 10;
		}while(number>0);
		System.out.println();
	}

}
