package demo04;

import java.util.Scanner;

public class demo043 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		Scanner in = new Scanner(System.in);
		for(int n=2;; n++)
		{
			int isPrime=1;
			for(int i=n-1;i>1;i++)
			{
				if(n%i==0)
				{
					isPrime=0;
					break;
				}
			}
			if(isPrime==1)
			{
				int primeNumber=0;
				System.out.print(n+" ");
				primeNumber++;//记录质数的个数
				if(primeNumber<=50)
				{
					System.out.println(n);
				}
				else
				{
					
				}
			}
		}
		
	}

}
