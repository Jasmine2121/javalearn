package demo05;

import java.util.Scanner;

public class demo053 {

	public static void main(String[] args) {
		//平均数
		Scanner in = new Scanner(System.in);
		int x;
		//创建数组
		int numbers= new int[100];
		double sum=0;
		int cnt=0;
		x= in.nextInt();
		while(x!=-1)
		{
			sum +=x;
			cnt++;
			x=in.nextInt();
		}
		if(cnt>0)
		{
			System.out.println(sum/cnt);
		}

	}

}
